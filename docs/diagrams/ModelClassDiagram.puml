@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam classBackgroundColor MODEL_COLOR

Package \nModel <<Rectangle>>{
Class "<<interface>>\nReadOnlyVms" as ReadOnlyVms
Class "<<interface>>\nReadOnlyUserPrefs" as ReadOnlyUserPrefs
Class "<<interface>>\nModel" as Model

Class Vms
Class ModelManager
Class UserPrefs

Class PatientManager
Class AppointmentManager
Class VaxTypeManager
Class KeywordManager
}

Class HiddenOutside #FFFFFF
HiddenOutside .down.> Model
Model .left.> ReadOnlyVms
Model .right.> ReadOnlyUserPrefs

ModelManager .up.|> Model
ModelManager -left-> "1" Vms
ModelManager -right-> "1" UserPrefs
Vms .up.|> ReadOnlyVms
UserPrefs .up.|> ReadOnlyUserPrefs

Vms *--> "1" PatientManager
Vms *--> "1" AppointmentManager
Vms *--> "1" VaxTypeManager
Vms *--> "1" KeywordManager

@enduml
