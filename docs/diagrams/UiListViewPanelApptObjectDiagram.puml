@startuml UiListViewPanelApptObjectDiagram
!include style.puml

skinparam RoundCorner 0

skinparam arrowThickness 1.1

hide <<hidden>> stereotype

<style>
    .hidden {
        BackGroundColor transparent
        BorderColor transparent
        FontColor transparent
        LineColor transparent
        StereotypeFontColor transparent
    }
</style>

Class "<u>appointmentListPanel:ListViewPanel</u>" as ListViewPanel UI_COLOR
Class "<u>displayFunction:BiFunction</u>" as BiFunction #000000
Class "<u>comparatorProperty:ObjectProperty</u>" as ComparatorProperty #000000
Class "<u>changeProperty:ObjectProperty</u>" as ChangeProperty #000000

Class "<u>:MapChangeListener</u>" as MapChangeListener #000000
Class "<u>:ObservableMap</u>" as ObservableMap #000000
Class "<u>:AppointmentManager</u>" as AppointmentManager LOGIC_COLOR

ListViewPanel -up-> ChangeProperty
ListViewPanel -right-> BiFunction
ListViewPanel -down-> ComparatorProperty

MapChangeListener -> ChangeProperty : updates >

ObservableMap -up-> MapChangeListener : updates >

AppointmentManager -up-> ObservableMap

@enduml
